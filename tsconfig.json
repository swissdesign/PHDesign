{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "skipLibCheck": true,
    "types": ["node"],
    "allowJs": true,
    "jsx": "react-jsx",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "allowImportingTsExtensions": true,
    "noEmit": true
  }
}
